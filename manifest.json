    "developer":
    {
        "name": "KamiKid_"
    },
    "icons":
    {
        "512": "icon.png"
    },
    "manifest_version": 3,
    "mod":
    {
        "license": "license.txt",
        "payload":
        {
            "background_music":
            [
                "music/track_1.mp3",
                "music/track_2.mp3",
